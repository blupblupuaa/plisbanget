# Antares IoT Configuration
# Dapatkan kredensial ini dari dashboard Antares platform.antares.id
ANTARES_API_KEY=your_api_key_here
ANTARES_APPLICATION_ID=your_application_id_here
ANTARES_BASE_URL=https://platform.antares.id:8443
ANTARES_DEVICE_ID=your_device_id_here

# Neon Database Configuration
# Dapatkan connection string dari https://neon.tech dashboard
DATABASE_URL=postgresql://username:password@ep-xxx.region.aws.neon.tech/database?sslmode=require

# Vercel Cron Secret (generate random string)
# Generate dengan: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
CRON_SECRET=your_cron_secret_here

# Application Configuration
NODE_ENV=production
PORT=5000